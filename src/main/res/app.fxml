<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER" prefHeight="600" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ainesh1998.Controller">

    <Region HBox.hgrow="ALWAYS" />

    <VBox alignment="CENTER" prefHeight="600" prefWidth="500">

        <Region VBox.vgrow="ALWAYS" />

        <Label fx:id="timerLabel" style="-fx-text-fill: WHITE; -fx-font-size: 40px; -fx-font-family: Courier" text="0.00" />

        <Region VBox.vgrow="ALWAYS" />

        <Canvas fx:id="cubeCanvas" height="400" width="500" />

        <Region VBox.vgrow="ALWAYS" />

        <TableView fx:id="statsTable" prefHeight="100" />

    </VBox>

    <Region HBox.hgrow="ALWAYS" />

    <ListView fx:id="timeList" prefWidth="100" />

</HBox>
